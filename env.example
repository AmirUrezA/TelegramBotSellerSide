# Copy this file to .env and fill the values

# Telegram Bot
BOT_TOKEN=your-telegram-bot-token
KAVENEGAR_API_KEY=your-kavenegar-api-key

# Postgres (used by both local and prod compose)
POSTGRES_DB=mybot
POSTGRES_USER=postgres
POSTGRES_PASSWORD=amir

# Optional override for direct connection (otherwise compose sets it)
# DATABASE_URL=postgresql+asyncpg://postgres:amir@db:5432/mybot

# For production compose image
# IMAGE_NAME=ghcr.io/owner/repo
# IMAGE_TAG=latest 